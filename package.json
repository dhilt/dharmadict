{
  "name": "dharmadict-frontend",
  "version": "1.0.1",
  "description": "Dharma Dictionary Front End",
  "scripts": {
    "start": "concurrently --kill-others \"npm run dev-server\" \"npm run prod-server\"",
    "build": "webpack --config webpack/prod.js --progress --colors -p",
    "dev-server": "node webpack/server.js",
    "prod-server": "node prod/server.js",
    "test": "mocha -R spec --bail",
    "test-frontend": "set NODE_ENV=\"test\" && jest --watch",
    "db-reset": "node prod/db/reset.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhilt/dharmadict.git"
  },
  "author": "dhilt <d.alexanov@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "ava": "^0.16.0",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.24.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.18.0",
    "jest": "^21.2.1",
    "json-loader": "^0.5.7",
    "localStorage": "^1.0.3",
    "mocha": "^3.5.0",
    "nock": "^9.0.22",
    "postcss-focus": "^1.0.0",
    "postcss-import": "^8.1.1",
    "postcss-loader": "^1.0.0",
    "postcss-reporter": "^2.0.0",
    "postcss-simple-vars": "^3.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "redux-ava": "^2.1.0",
    "redux-mock-store": "^1.3.0",
    "snazzy": "^5.0.0",
    "standard": "^8.2.0",
    "style-loader": "^0.13.1",
    "surge": "^0.18.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "bcryptjs": "^2.3.0",
    "btoa": "^1.1.2",
    "react": "^15.1.0",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.1.0",
    "react-intl": "^2.4.0",
    "react-modal": "^1.5.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.2",
    "redux": "^3.5.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-runtime",
      "transform-object-rest-spread"
    ]
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
}
